<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>Disable</Nullable>
  </PropertyGroup>
	<PropertyGroup>
		<PackageId>ReactiveDAG</PackageId>
		<Version>1.0.3</Version>
		<Authors>Richard Smythe</Authors>
		<Description>A tool for managing dynamic computations in a directed acyclic graph, allowing automatic updates of node values based on input changes.</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/richardsmythe/ReactiveDAG</PackageProjectUrl>
		<RepositoryUrl>https://github.com/richardsmythe/ReactiveDAG</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<IncludeSymbols>true</IncludeSymbols>
		<TargetFramework>net8.0</TargetFramework>
		<PackageTags>dag;directed acyclic graph; data structure; reactive; data flow; computation; graph; node; workflow</PackageTags>
	</PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
